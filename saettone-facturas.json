{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "consorcio": { "type": "string" },
      "fecha": { "type": "string", "format": "date" },
      "proveedor": { "type": "string" },
      "tipo_comprobante": { "type": "string", "enum": ["A", "B", "C"] },
      "punto_venta": { "type": "string" },
      "nro_comprobante": { "type": "string" },
      "detalle": { "type": "string" },
      "subtotal": { "type": "string" }
    },
    "required": [
      "consorcio",
      "fecha",
      "proveedor",
      "tipo_comprobante",
      "punto_venta",
      "nro_comprobante",
      "detalle",
      "subtotal"
    ],
    "additionalProperties": false
  }
}
